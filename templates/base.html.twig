<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />

    <!-- App favicon -->
    <link rel="shortcut icon" href="{{ asset('coderthemes.com/osen/layouts/assets/images/favicon.ico') }}">

    <!-- Theme Config Js -->
    <script src="{{ asset('coderthemes.com/osen/layouts/assets/js/config.js') }}"></script>
    <script src="{{ asset('coderthemes.com/osen/layouts/assets/js/vendor.min.js') }}"></script>

    <!-- App js -->
    <script src="{{ asset('coderthemes.com/osen/layouts/assets/js/app.js') }}"></script>

    <!-- Vendor css -->
    <link href="{{ asset('coderthemes.com/osen/layouts/assets/css/vendor.min.css') }}" rel="stylesheet" type="text/css" />

    <!-- App css -->
    <link href="{{ asset('coderthemes.com/osen/layouts/assets/css/app.min.css') }}" rel="stylesheet" type="text/css" id="app-style" />

    <!-- Icons css -->
    <link href="{{ asset('coderthemes.com/osen/layouts/assets/css/icons.min.css') }}" rel="stylesheet" type="text/css" />
    {% block css %}{% endblock %}
</head>

<body>
    <!-- Begin page -->
    <div class="wrapper">

        <!-- Sidenav Menu Start -->
        <div class="sidenav-menu">
            <!-- Brand Logo -->
            <a href="index.html" class="logo">
                <span class="logo-light">
                    <span class="logo-lg"><img src="{{ asset('coderthemes.com/osen/layouts/assets/images/logo1.png') }}" alt="logo" style="height: 55px"></span>
                    <span class="logo-sm"><img src="{{ asset('coderthemes.com/osen/layouts/assets/images/logo-sm.png') }}" alt="small logo"></span>
                </span>

                <span class="logo-dark">
                    <span class="logo-lg"><img src="{{ asset('coderthemes.com/osen/layouts/assets/images/logo-dark.png') }}" alt="dark logo"></span>
                    <span class="logo-sm"><img src="{{ asset('coderthemes.com/osen/layouts/assets/images/logo-sm.png') }}" alt="small logo"></span>
                </span>
            </a>

            <!-- Sidebar Hover Menu Toggle Button -->
            <button class="button-sm-hover">
                <i class="ti ti-circle align-middle"></i>
            </button>

            <!-- Full Sidebar Menu Close Button -->
            <button class="button-close-fullsidebar">
                <i class="ti ti-x align-middle"></i>
            </button>

            <div data-simplebar>
                <!--- Sidenav Menu -->
                <ul class="side-nav">
                    <li class="side-nav-title">Dash</li>
                    <!-- ... existing menu items ... -->
                </ul>
            </div>
        </div>
        <!-- Sidenav Menu End -->

        <!-- Content -->
        <div class="page-content">
            {% block body %}{% endblock %}
        </div>

        <!-- Footer Start -->
        <footer class="footer">
            <div class="page-container">
                <div class="row">
                    <div class="col-md-6 text-center text-md-start">
                        <script>document.write(new Date().getFullYear())</script> Â© Osen - By <span class="fw-bold text-decoration-underline text-uppercase text-reset fs-12">Coderthemes</span>
                    </div>
                    <div class="col-md-6">
                        <div class="text-md-end footer-links d-none d-md-block">
                            <a href="javascript: void(0);">About</a>
                            <a href="javascript: void(0);">Support</a>
                            <a href="javascript: void(0);">Contact Us</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- end Footer -->

    </div>

    <!-- JAVASCRIPT -->
    <script src="{{ asset('coderthemes/osen/assets/libs/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/libs/simplebar/simplebar.min.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/libs/node-waves/waves.min.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/libs/feather-icons/feather.min.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/js/pages/plugins/lord-icon-2.1.0.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/js/plugins.js') }}"></script>
    <script src="{{ asset('coderthemes/osen/assets/js/app.js') }}"></script>

    {% block js %}{% endblock %}
</body>
</html>